

<%= render "/layouts/nav" %>
<br></br>
<div class="container-fluid text-center">
  <div class="row content">
    <div class="col-sm-2 sidenav" id="colored">
      <p><a href="#">Link</a></p>
      <p><a href="#">Link</a></p>
    </div>
    <div class="col-sm-8 text-left">
      <h3>List of Your Patients</h3>
      <br></br>
      <div id="patient_list">
          <ul><% @user.patients.each do |patient|%>
          <a href="/users/<%=patient.user.id%>/patients/<%=patient.id%>" class="buttons"><%=patient.name%></a>
          <br></br>
          <%end%></ul>
      </div>
    </div>
    <div class="col-sm-2 sidenav">


          <%= form_tag patients_path, multipart: true do %>
          <%= file_field_tag :leads %>
          <%= submit_tag "Upload CSV File" %>
          <%end%>

    </div>
  </div>
</div>



<footer class="container-fluid text-center">
  <p>Contact Us</p>
</footer
